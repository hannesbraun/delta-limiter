@prefix doap:	<http://usefulinc.com/ns/doap#> .
@prefix lv2:	<http://lv2plug.in/ns/lv2core#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix units:	<http://lv2plug.in/ns/extensions/units#> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .

<https://github.com/hannesbraun>
	a foaf:Person ;
	foaf:name "Hannes Braun" ;
	foaf:mbox <mailto:hannesbraun@mail.de> ;
	foaf:homepage <http://hannesbraun.github.io/> .

<https://github.com/hannesbraun/lv2plugins>
	a doap:Project ;
	doap:maintainer <https://github.com/hannesbraun> ;
	doap:name "Hannes Braun's Collection of LV2 Plugins" .

<https://github.com/hannesbraun/delta-limiter>
	a lv2:Plugin ,
		lv2:DistortionPlugin ,
		lv2:LowpassPlugin ;
	lv2:project <https://github.com/hannesbraun/lv2plugins> ;
	doap:name "Delta Limiter" ;
	doap:license <https://opensource.org/licenses/none> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 0 ;
		lv2:symbol "in0" ;
		lv2:name "InL"
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 1 ;
		lv2:symbol "in1" ;
		lv2:name "InR"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 2 ;
		lv2:symbol "out0" ;
		lv2:name "OutL"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 3 ;
		lv2:symbol "out1" ;
		lv2:name "OutR"
	]  , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 4 ;
		lv2:symbol "inGain" ;
		lv2:name "Input Gain" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 4.0 ;
		units:unit units:coef
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 5 ;
		lv2:symbol "limit" ;
		lv2:name "Delta Limit" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 6 ;
		lv2:symbol "outGain" ;
		lv2:name "Output Gain" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 4.0 ;
		units:unit units:coef
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ;
		lv2:index 7 ;
		lv2:symbol "dryWet" ;
		lv2:name "Dry/Wet" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0
	] .
